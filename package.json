    "unit": "jest test/unit/specs --coverage",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "build:iview": "cross-env component=iview node build/build.js ",
    "build:element": "cross-env component=element node build/build.js ",
    "build:to": "node build/buildTo.js",
    "build:prodLib": "node build/buildProdLib.js"
  },
  "dependencies": {
